#include <iostream>
using namespace std;

// Задача 1
int task1() {
	// задание переменной скорости
	double v = 0.00;
	cin >> v;

	// Высчитывание вывода
	if (v < 60) {
		cout << "Скорость в пределах нормы" << endl;
	} else if (60 <= v && v <= 80 ) {
		cout << "Вы приближаетесь к превышению скорости" << endl;
	} else {
		cout << "Превышение скорости!" << endl;
	}

	return 0;
}

// Задача 2
int task2() {
	// ввод переменных
	int res = 1, n = 1;
	cin >> n;

	// высчитывание факториала
	int i = 1;
	while (i <= n) {
		res *= i;
		i++;
	}

	//вывод факториала
	cout << res << endl;

	return 0;
}

// запуск задач
int main() {
	task1();
	task2();
	return 0;
}

